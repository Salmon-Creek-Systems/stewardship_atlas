     "icon-size": 2.0,
     "interaction": "interface",
     "editable_columns": [
         {"name": "name", "type": "string", "default": "" },
         {
             "name": "accessible",
             "type": "radio",
             "values": [ 
                 {"accessible": "yes"},
		 {"accessible": "no"}
             ],
             "default":  {"accessible": "no"}
         },
	 {
             "name": "size",
             "type": "radio",
             "values": [ 
                 {"size": "> 500k gals.", "capacity": 500 },
		 {"size": "100k-500k gals", "capacity": 100},
		 {"size": "10k-100k gals", "capacity": 10},
		 {"size": "< 10k gals", "capacity": 0}
             ],
             "default": {"size": "10k-100k gals", "capacity": 10}
	 }
     
     ]
    },
    {"name": "buildings", "geometry_type": "polygon", "color": [240, 0, 0], "fill_color": [250,0,0],
     "add_labels": true, "interaction": "interface"},
    {"name": "addresses", "geometry_type": "polygon", "color": [255, 0, 0]},
    {"name": "parcels", "geometry_type": "polygon", "color": [255, 0, 0, 0.3], "fill_color": [0,0,0,0]}
