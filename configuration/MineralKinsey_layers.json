[
    {"name": "regions", "geometry_type": "polygon", "color": [50, 50, 50], "interaction": "interface", "access": ["admin"]},
    {"name": "elevation", "geometry_type": "raster"},
    {"name": "contours", "geometry_type": "linestring", "color": [50, 200, 50],
     "vis" : {"minzoom": 14, "maxzoom": 22}},
    {"name": "basemap", "geometry_type": "raster"},
    {"name": "lidar_basemap", "geometry_type": "raster"},
    {"name": "roads", "geometry_type": "linestring", 
        "color": [100, 55, 50], "add_labels": true, "interaction": "interface", "vector_width":true,
        "editable_columns": [
            {"name": "name", "type": "string", "default": "Road" },
            {
                "name": "STREETTYPE",
                "type": "radio",
                "values": [ 
                    {"STREETTYPE": "RD", "vector_width": 15},
                    {"STREETTYPE": "ST", "vector_width": 13},
                    {"STREETTYPE": "Other", "vector_width": 10}
                ],
              "default": {"STREETTYPE": "RD", "vector_width": 9}
            }]},
    {"name": "roads_index", "geometry_type": "polygon", "color": [100, 55, 50], "fill_color": [100, 55, 50], "fill_opacity": 0.3,"add_labels": false, "access": ["internal", "admin"]},
    
    {"name": "internal_roads", "geometry_type": "linestring", "color": [255, 155, 50],
     "add_labels": true, "access": [ "admin"], "vector_width":true},
    {"name": "burns_index", "geometry_type": "polygon", "color": [0,0,0], "fill_color": [200, 55, 55], "fill_opacity": 0.6, "add_labels": true, "access": ["internal", "public", "admin"]},
    {"name": "creeks_index", "geometry_type": "polygon", "color": [50, 55, 255], "fill_color": [50, 55, 255], "fill_opacity": 0.3,"add_labels": false, "access": ["internal", "public", "admin"]},
    {"name": "creeks", "geometry_type": "linestring", "add_labels": true, "color": [50, 50, 200],
     "interaction": "interface", "vector_width":true},
    {"name": "hydrants", "geometry_type": "point", "color": [0, 0, 255], "add_labels": true,
     "symbol": {"png": "hydrant.png", "icon": "extra/half-circle"},
     "icon-size": 1.0,
     "interaction": "interface"},
    {"name": "ponds", "geometry_type": "polygon", "fill_color": [50, 50, 250]},
    {"name": "natural", "geometry_type": "point", "color": [0, 200, 55], "add_labels": true,
     "symbol": {"png": "pin.png", "icon": "custom/octy"},
     "icon-size": 1.0, 
     "interaction": "interface"},    
    {"name": "buildings", "geometry_type": "polygon", "color": [0, 0, 0], "fill_color": [100,100,100],
     "add_labels": true, "interaction": "interface"},
    {"name": "addresses", "geometry_type": "polygon", "color": [255, 0, 0]},
    {"name": "parcels", "geometry_type": "polygon", "color": [255, 0, 0, 0.3], "fill_color": [0,0,0,0]},
    {"name": "burns", "geometry_type": "polygon", "color": [155, 5, 5], "fill_color": [255, 50,50],
     "add_labels": true,  "interaction": "interface","access": [ "admin"],
     "paint" : {"fill-opacity": 0.75},
     "editable_columns": [
         {"name": "name", "type": "string", "default": "Road" },
         {"name": "readiness",
          "type": "radio",
          "values": [ 
              {"readiness": "low", "fill_color": [200,100, 100]},
	      {"readiness": "med", "fill_color": [100,200, 100]},
              {"readiness": "high", "fill_color": [100,100, 200]}],
          "default": {"readiness": "med", "fill_color": [100,200, 100]}
	 }   
     ]
    }
]
