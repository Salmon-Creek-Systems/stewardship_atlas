    "humtrans_roads" : {
	"name": "roads",
        "asset_type": "inlet", 
        "interaction": "download",
        "fetch_type": "local_ogr",
	"geometry": "dataswale",
        "data_type": "geojson",
	"feature_type": "line",
	"alterations": {
            "canonicalize": [
		{
		    "to": "REMOVE",
		    "from": [
			"ID"
		    ],
		    "remove_prefix": []
		},
		{
		    "to": "name",
		    "from": [
			"FULLRDNAME"
		    ],
		    "remove_prefix": [
			"PRIVATE",
			"UNN",
			"GATED"
		    ]
		},
		{
		    "to": "STREETTYPE",
		    "from": [
			"STREETTYPE"
		    ],
		    "remove_prefix": []
		}
            ],
            "vector_width": {
		"attribute": "STREETTYPE",
		"map": {
		    "RD": 6,
		    "DR": 3
		},
		"default": 1
            }
	},
	"inpath_template": "humtranssp27.shp",
