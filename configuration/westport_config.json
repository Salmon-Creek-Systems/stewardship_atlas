{"config_sources": {
    "bbox": "bbox_config.json",
    "inlets": "inlets_config.json"

},
 "data_root": "/root/swales",
 "name": "westport",
 "description": "Westport VFD",
 "dataswale": {
     "crs": "EPSG:4269",     
     "bbox": "$bbox::westportvfd",
     "versions": ["staging"],
     "layers": [
	 {"name": "roads", "datatype": "vector"},
	 {"name": "creeks", "datatype": "vector"}
	 ]
 },
 "assets":{
     "dem": {
         "out_layer": "dem",
         "config": "$inlets::opentopo_dem"
     },
     "public_roads" : {
	 "out_layer": "roads",
	 "config": "$inlets::overture_roads"
     },
     "public_creeks" : {
         "out_layer": "creeks",
         "config": "$inlets::nhd_creeks"
     }
 }
}


