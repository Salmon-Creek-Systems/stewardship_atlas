{"config_sources": {
    "bbox": "bbox_config.json",
    "inlets": "inlets_config.json"

},
 "data_root": "/root/swales",
 "name": "westport",
 "description": "Westport VFD",
 "dataswale": {
     "crs": "EPSG:4269",     
     "bbox": "$bbox::westportvfd",
     "versions": ["staging"],
     "layers": [
	 {"name": "basemap", "geometry_type": "raster"},
	 {"name": "roads", "geometry_type": "linestring"},
	 {"name": "creeks", "geometry_type": "linestring"}
	 ]
 },
 "assets":{
     "dem": {
         "out_layer": "dem",
         "config": "$inlets::opentopo_dem"
     },
     "public_roads" : {
	 "out_layer": "roads",
	 "config": "$inlets::overture_roads"
     },
     "public_creeks" : {
         "out_layer": "creeks",
         "config": "$inlets::nhd_creeks"
     },
     "local_hillshade" : {
         "out_layer": "basemap",
         "config": "$inlets::local_hillshade"
     },
     "webmap" : {
        "in_layers": ["basemap", "roads", "creeks"],
        "config": "$inlets::webmap"
     }
 }
}


